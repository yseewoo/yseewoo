void setup(){
 size(500,500);
 strokeWeight(30);
 }

void draw(){
 background(200);
 stroke(255);
 line(40,0,70,height);
 if(mousePressed){
   if(mouseButton==CENTER){
     stroke(0);
 }
 else{
   stroke(255,0,0);
 }
 }
 
line(0,70,width,50);
}
